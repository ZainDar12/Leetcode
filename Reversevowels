char* reverseVowels(char* s) {
bool isVowel(char c) {
    c = tolower(c);
    return (c=='a'||c=='e'||c=='i'||c=='o'||c=='u');
}
    int l = 0, r = strlen(s) - 1;

    while (l < r) {
        while (l < r && !isVowel(s[l])) l++;
        while (l < r && !isVowel(s[r])) r--;

        char temp = s[l];
        s[l] = s[r];
        s[r] = temp;

        l++;
        r--;
    }
    return s;
}
