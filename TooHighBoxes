#include <stdio.h>
#include <stdlib.h>
#define MAX_HEIGHT 41
struct box {
    int length;
    int width;
    int height;
};
int volume(struct box b) {
    return b.length * b.width * b.height;
}
int main() {
    int n;
    scanf("%d", &n);
    struct box *boxes = malloc(n * sizeof(struct box));
    for (int i = 0; i < n; i++)
        scanf("%d %d %d", &boxes[i].length, &boxes[i].width, &boxes[i].height);
    for (int i = 0; i < n; i++)
        if (boxes[i].height < MAX_HEIGHT)
            printf("%d\n", volume(boxes[i]));
    free(boxes);
    return 0;
}
